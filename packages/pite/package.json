{
    "name": "@naverpay/pite",
    "author": "@NaverPayDev/frontend",
    "version": "0.0.1",
    "description": "vite 번들러 패키지",
    "main": "./cjs/index.js",
    "module": "./esm/index.js",
    "types": "./cjs/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./esm/index.d.ts",
                "default": "./esm/index.js"
            },
            "require": {
                "types": "./cjs/index.d.ts",
                "default": "./cjs/index.js"
            }
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "tsc": "tsc",
        "copy": "cp package.json dist/ && cp README.md dist/",
        "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json && pnpm run copy",
        "clean": "rm -rf ./dist"
    },
    "dependencies": {
        "browserslist": "^4.24.2",
        "browserslist-to-esbuild": "^2.1.1",
        "vite-plugin-dts": "^4.3.0"
    },
    "peerDependencies": {
        "vite": "^5.4.11"
    }
}
